def ge(A, b):
    m = len(A)      # store number of sublists of A rows
    if matrix(A) == True and detect_list(A) == False and len(b) == m:       # checks if list is matrix compatible, there are no order higher than 2 of list of lists and the length of b is length of A
        n = len(A[0])       # gives the number of columns of A
        C = [[0]*n]*m       # initailizes a list of lists of all zeros 
        k = 0
        for i in range(n):
            for j in range(1, m)
                t = A[j - k - 1][i]
                C[j][i] = - (A[j][i] / t)
                k = k + 1
            

            
    else: 
        return "Choose a valid input"
